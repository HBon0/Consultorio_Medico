@model Consultorio_Medico.BL.DTOs.WorkPlaceDTO.WorkPlaceInputDTO

@{
    ViewData["Title"] = "Edit";
}
<h4><center>Editar Lugar de Trabajo</center></h4>
<hr />

<div class="container" style="padding: 50px 10px 10px 10px">
    <div class="">
        <div class="row d-flex justify-content-center align-items-center">

            <div class="col-md-4">
                <form asp-action="Edit">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group">
                        <label asp-for="WorkPlacesId" class="control-label" hidden></label>
                        <input asp-for="WorkPlacesId" class="form-control" hidden />
                        <span asp-validation-for="WorkPlacesId" class="text-danger" hidden></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="WorkPlaces" class="control-label"></label>
                        <input asp-for="WorkPlaces" class="form-control" />
                        <span asp-validation-for="WorkPlaces" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <input type="submit" value="Edit" class="btn btn-primary" /> ||
                        <a asp-action="Index" class="btn btn-danger">Regresar a lista</a>
                    </div>
                    
                </form>
            </div>

        </div>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
